{
  "name": "react-select-currency",
  "version": "1.0.3",
  "description": "Select currency from a dropdown with auto-fill.",
  "keywords": [
    "react",
    "reactjs",
    "i18n",
    "currency",
    "component"
  ],
  "homepage": "https://github.com/lsiden/react-select-currency",
  "main": "dist/bundle.js",
  "author": "Lawrence Siden",
  "license": "MIT",
  "scripts": {
    "test": "DEBUG=select-currency:* DEBUG_DEPTH=5 NODE_ENV=test jest",
    "test:watch": "DEBUG=select-currency:* DEBUG_DEPTH=5 NODE_ENV=test yarn test --watch",
    "start": "NODE_ENV=dev webpack-dev-server --config webpack.dev.js --hot --inline --open",
    "cloc": "cloc $(git ls-files)",
    "lint": "eslint src/",
    "clean": "rimraf gh-pages dist",
    "prepare": "yarn clean; NODE_ENV=production webpack --config webpack.prod.js",
    "prepare:demo": "yarn clean; NODE_ENV=development webpack --config webpack.demo.js",
    "gh-pages": "yarn prepare:demo; node ./prepare-gh-pages.js; gh-pages -d gh-pages"
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "classnames": "^2.2.5",
    "country-code": "^0.0.3",
    "css-loader": "^0.28.7",
    "cuid": "^1.3.8",
    "file-loader": "^1.1.5",
    "flag-icon-css": "^2.8.0",
    "locale-currency": "^0.0.1",
    "react": "^16.0.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.0.0",
    "style-loader": "^0.19.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cloc": "^2.3.2",
    "enzyme": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.0.0",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
